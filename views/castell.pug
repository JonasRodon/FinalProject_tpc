              section.card
                h1 Denominació i catalogació del castell
                ul.nav.nav-tabs
                  li.active
                    a(data-toggle='tab', href='#home') estructura
                  li
                    a(data-toggle='tab', href='#menu1') pisos d’alçada
                  li
                    a(data-toggle='tab', href='#menu2') tipus d’estructura
                  li
                    a(data-toggle='tab', href='#menu3') tècnica de construcció
                  li
                    a(data-toggle='tab', href='#menu4') gamma
                .tab-content(style='color:black;')
                  #home.tab-pane.fade.in.active
                    h3 #{c.details.esName}
                    p #{c.details.esDescription}
                  #menu1.tab-pane.fade
                    h3 #{c.details.height}
                    p #{c.details.esHeightDescription}
                  #menu2.tab-pane.fade
                    h3 #{c.details.esType}
                    p #{c.details.esTypeDescription}
                  #menu3.tab-pane.fade
                    h3 #{c.details.consTec}
                    p
                  #menu4.tab-pane.fade
                    h3 #{c.details.gamma}
                    p
              section.card
                h1 Parts del castell
                ul.nav.nav-tabs
                  li.active
                    a(data-toggle='tab', href='#menu5') pinya
                  li
                    a(data-toggle='tab', href='#menu6') tronc
                  li
                    a(data-toggle='tab', href='#menu7') pom de dalt
                  li
                    a(data-toggle='tab', href='#menu8') folre
                  li
                    a(data-toggle='tab', href='#menu9') manilles
                .tab-content(style='color:black;')
                  #menu5.tab-pane.fade.in.active
                    h3
                      | #{c.parts.pinya} 
                    p
                  #menu6.tab-pane.fade
                    h3
                      | #{c.parts.tronc} 
                    p
                  #menu7.tab-pane.fade
                    h3
                      | #{c.parts.pom} 
                    p
                  #menu8.tab-pane.fade
                    h3
                      | #{c.parts.folre} 
                    p
                  #menu9.tab-pane.fade
                    h3
                      | #{c.parts.manilles} 
                    p
              section.card
                h1 Taula de puntuacions del Concurs de Castells 2016
                ul.nav.nav-tabs
                  li.active
                    a(data-toggle='tab', href='#menu10') punts carregat
                  li
                    a(data-toggle='tab', href='#menu11') punts descarregat
                  li
                    a(data-toggle='tab', href='#menu12') grup
                  li
                    a(data-toggle='tab', href='#menu13') subgrup
                .tab-content(style='color:black;')
                  #menu10.tab-pane.fade.in.active
                    h3 #{c.score.Pointscar}
                    p
                  #menu11.tab-pane.fade
                    h3 #{c.score.Pointsdes}
                    p
                  #menu12.tab-pane.fade
                    h3 #{c.score.group}
                    p
                  #menu13.tab-pane.fade
                    h3 #{c.score.subGroup}
                    p
              section.card
                h1 Estadística
                ul.nav.nav-tabs
                  li.active
                    a(data-toggle='tab', href='#menu14') núm. de colles que han descarregat
                  li
                    a(data-toggle='tab', href='#menu15') núm. de colles que l’han carregat
                  li
                    a(data-toggle='tab', href='#menu16') núm. de colles que l’han intentat
                .tab-content(style='color:black;')
                  #menu14.tab-pane.fade.in.active
