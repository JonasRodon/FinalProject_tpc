                    p
                  #menu22.tab-pane.fade
                    h3 #{c.hNew.firstCollCar}
                    p
                  #menu23.tab-pane.fade
                    h3 #{c.hNew.firstCollDes}
                    p
                  #menu24.tab-pane.fade
                    h3 #{c.hNew.firstCollDes}
                    p
              section.card
                ul.imgSlider
                  li
                    img(id='#{c.imgScr.number}', width='400px ', src='#{c.imgScr.src} ', alt='#{c.imgScr.name} ')
                ul.vigSlider
